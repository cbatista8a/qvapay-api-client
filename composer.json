{
    "name": "cbatista8a/qvapay-api-client",
    "version": "1.0.0",
    "description": "QvaPay PHP Api Client",
    "type": "package",
    "keywords": [
        "cubadevops",
        "qvapay",
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://qvapay.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Batista",
            "email": "cbatista8a@gmail.com",
            "homepage": "https://cuba-devops.online"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "QvaPay\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "QvaPay\\Test\\" : "test/" }
    }
}
